                            <Menu>
                                <MenuItem Header="保存" Name="Save1" Click="saveBaseInfo_Click" IsEnabled="{Binding ElementName=workFlowControl1, Path=CanSave}">
                                    <MenuItem.Icon>
                                        <Image Source="/Logging_APP;component/Images/filesave.png" />
                                    </MenuItem.Icon>
                                </MenuItem>
                            </Menu>
                            <Grid>
