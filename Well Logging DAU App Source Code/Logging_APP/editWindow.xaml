<gui:BaseWindow x:Class="Logging_App.editWindow"
        xmlns:gui="clr-namespace:Logging_App.GUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="编辑" Height="350" Width="450" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <TextBox Margin="3" Name="textbox1" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto"></TextBox>
        <Canvas Grid.Row="1">
            <Button Canvas.Left="104" Canvas.Top="3" Content="确定" Height="23" Name="button1" Width="57" Click="button1_Click" />
            <Button Canvas.Left="245" Canvas.Top="3" Content="取消" Height="23" Name="button2" Width="57" Click="button2_Click" />
        </Canvas>
    </Grid>
</gui:BaseWindow>
